{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "includeFiles": "public/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "API_VERSION": "1.0.0",
    "LOG_LEVEL": "DEBUG"
  }
}
